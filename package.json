{
  "name": "angular-startup",
  "version": "1.0.0",
  "description": "startup project",
  "main": "index.html",
  "scripts": {
    "init-web-server-dev": "babel-node build-scripts/demo-webserver-dev.js",
    "lint": "esw webpack.config.* src build-scripts --color",
    "lint-watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "prestart": "babel-node build-scripts/notify-app-start.js",
    "start": "npm-run-all --parallel init-web-server-dev security-check lint-watch test-watch",
    "test": "mocha --opts mocha.opts",
    "test-coverage":"istanbul cover node_modules/mocha/bin/_mocha -- --opts mocha.opts && start chrome coverage/lcov-report/index.html ",
    "test-watch": "npm run test -- --watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node build-scripts/build-prod.js",
    "postbuild": "babel-node build-scripts/demo-webserver-prod.js"
  },
  "author": "Prax",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.14.1",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.10.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "nsp": "^2.6.2",
    "open": "0.0.5",
    "path": "^0.12.7",
    "strip-loader": "^0.1.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-md5-hash": "0.0.5"
  }
}
